{
  "$schema":"https:\/\/schemas.wp.org\/trunk\/theme.json",
  "version":2,
  "settings":{
    "appearanceTools":true,
    "color":{
      "palette":[
        {
          "slug":"primary",
          "color":"var(--bs-primary)",
          "name":"Primary"
        },
        {
          "slug":"secondary",
          "color":"var(--bs-secondary)",
          "name":"Secondary"
        },
        {
          "slug":"success",
          "color":"var(--bs-success)",
          "name":"Success"
        },
        {
          "slug":"info",
          "color":"var(--bs-info)",
          "name":"Info"
        },
        {
          "slug":"warning",
          "color":"var(--bs-warning)",
          "name":"Warning"
        },
        {
          "slug":"danger",
          "color":"var(--bs-danger)",
          "name":"Danger"
        },
        {
          "slug":"light",
          "color":"var(--bs-light)",
          "name":"Light"
        },
        {
          "slug":"dark",
          "color":"var(--bs-dark)",
          "name":"Dark"
        }
      ]
    },
    "spacing":[
      {
        "units":[
          "px",
          "em",
          "rem",
          "vh",
          "vw"
        ]
      }
    ],
    "layout":{
      "contentSize":"840px",
      "wideSize":"1100px"
    }
  }
}